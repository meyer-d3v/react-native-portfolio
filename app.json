{
  "expo": {
    "name": "Meyer-dev",
    "slug": "Meyer-dev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/meyer-dev.png",
    "scheme": "meyerdev",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/meyer-dev.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.deanmeyer.Meyerdev"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/meyer-dev.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f2434dd6-ce06-4dd8-87eb-43ba6ab184ed"
      }
    }
  }
}
